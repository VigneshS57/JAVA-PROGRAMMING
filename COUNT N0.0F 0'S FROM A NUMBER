import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int count=0;
      int a=n;
      while(a>0){
        int c=a%10;
        a/=10;
        if(c==0){
          count++;
        }
      }
      System.out.println(count);
  }
}
