import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt(),temp;
       temp=n;
      int rem,res=0;
      while(n!=0){
        rem=n%10;
        res+=Math.pow(rem,3);
        n/=10;
      }
      if(res==temp){
      System.out.print("YES");
      }
      else
      System.out.println("NO");
  }
}
