import java.util.*;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
     String s=sc.next();
    char ch[]=s.toCharArray();
    for(int i=0;i<ch.length;i++){
      if(ch[i]>='A' && ch[i]<='Z'){
        int val=((int)ch[i])+32;
        char asd=(char)val;
        System.out.print(asd);
      }
      else{
      int val=((int)ch[i])-32;
       char asd=(char)val;
        System.out.print(asd);
    }
    }
  }
}
