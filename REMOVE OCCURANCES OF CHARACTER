import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      String s=sc.next();
      char x=sc.next().charAt(0);
      char ch[]=s.toCharArray();
      for(int i=0; i<ch.length;i++){
         if(!(ch[i]==x)){
          System.out.print(ch[i]);
        }
      }
      
}}
