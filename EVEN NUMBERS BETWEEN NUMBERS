//PRINT ALL EVEN NUUMBERS BETWEEN 2NUMBERS
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int a=sc.nextInt();
      int b=sc.nextInt();
  
      for(int i=a+1;i<b;i++){
        if(i%2==0){
          System.out.println(i);
        }
      
      }
  }
}
