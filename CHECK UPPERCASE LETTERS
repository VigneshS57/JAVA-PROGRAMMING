import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      String s=sc.next();
      int count=0;
      char ch[]=s.toCharArray();
      for(int i=0;i<ch.length-1;i++){
        if(!(ch[i]>='A' && ch[i]<='Z')){
          count++;
          break;
        }
        else
          count=0;
      }
      if(count==0){
        System.out.println("true");
      }
      else
        System.out.println("false");
  }
}
